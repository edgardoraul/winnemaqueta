@import "variables"

@font-face
	font-family 'icomoon'
	src url(icomoon-font-path + '/icomoon.eot?9r76sl')
	src url(icomoon-font-path + '/icomoon.eot?9r76sl#iefix') format('embedded-opentype'), url(icomoon-font-path + '/icomoon.ttf?9r76sl') format('truetype'), url(icomoon-font-path + '/icomoon.woff?9r76sl') format('woff'), url(icomoon-font-path + '/icomoon.svg?9r76sl#icomoon') format('svg')
	font-weight normal
	font-style normal
	font-display block

i
	font-family 'icomoon' !important
	speak never
	font-style normal
	font-weight normal
	font-variant normal
	text-transform none
	line-height 1

	/* Better Font Rendering =========== */
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale

